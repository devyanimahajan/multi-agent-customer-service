import subprocess, sys, time

HOST = "127.0.0.1"
DATA_PORT, SUPPORT_PORT, ROUTER_PORT = 8001, 8002, 8003

procs = {}
def start(name, app_ref, port):
    cmd = [sys.executable, "-m", "uvicorn", app_ref, "--host", HOST, "--port", str(port), "--log-level", "info"]
    p = subprocess.Popen(cmd, stdout=subprocess.PIPE, stderr=subprocess.PIPE, text=True)
    procs[name] = p
    return p

start("data", "agents.data_agent_server:app", DATA_PORT)
start("support", "agents.support_agent_server:app", SUPPORT_PORT)
start("router", "agents.router_agent_server:app", ROUTER_PORT)

time.sleep(1.0)
{k: p.pid for k, p in procs.items()}



import time
time.sleep(2)



def full_stderr(name: str, max_chars: int = 20000):
    p = procs[name]
    if p.stderr is None:
        print("No stderr pipe.")
        return
    txt = p.stderr.read()
    if not txt:
        print("(empty stderr)")
        return
    print(txt[-max_chars:])

print("DATA FULL STDERR:")
full_stderr("data")



import asyncio, httpx, time
from a2a.types import AgentCard
from a2a.utils.constants import AGENT_CARD_WELL_KNOWN_PATH

DATA_URL = f"http://{HOST}:{DATA_PORT}"
SUPPORT_URL = f"http://{HOST}:{SUPPORT_PORT}"
ROUTER_URL = f"http://{HOST}:{ROUTER_PORT}"

async def wait_for_card(base_url: str, timeout_s: float = 20.0) -> AgentCard:
    url = base_url.rstrip("/") + AGENT_CARD_WELL_KNOWN_PATH
    deadline = time.time() + timeout_s
    last_err = None
    async with httpx.AsyncClient(timeout=2.0) as c:
        while time.time() < deadline:
            try:
                r = await c.get(url)
                if r.status_code == 200:
                    return AgentCard.model_validate(r.json())
            except Exception as e:
                last_err = e
            await asyncio.sleep(0.25)
    raise RuntimeError(f"Agent card not reachable: {url}. Last error: {last_err}")

data_card = await wait_for_card(DATA_URL)
support_card = await wait_for_card(SUPPORT_URL)
router_card = await wait_for_card(ROUTER_URL)

print("OK:", data_card.name, support_card.name, router_card.name)




